<p id="notice"><%= notice %></p>

<h1>Listing Forms Responses</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Value</th>
      <th>Field type</th>
      <th>Submission</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forms_responses.each do |forms_response| %>
      <tr>
        <td><%= forms_response.question %></td>
        <td><%= forms_response.value %></td>
        <td><%= forms_response.field_type %></td>
        <td><%= forms_response.submission %></td>
        <td><%= link_to 'Show', forms_response %></td>
        <td><%= link_to 'Edit', edit_forms_response_path(forms_response) %></td>
        <td><%= link_to 'Destroy', forms_response, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Response', new_forms_response_path %>
