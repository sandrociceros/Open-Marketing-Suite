<p id="notice"><%= notice %></p>

<h1>Listing Forms Entries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forms_entries.each do |forms_entry| %>
      <tr>
        <td><%= link_to 'Show', forms_entry %></td>
        <td><%= link_to 'Edit', edit_forms_entry_path(forms_entry) %></td>
        <td><%= link_to 'Destroy', forms_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entry', new_forms_entry_path %>
